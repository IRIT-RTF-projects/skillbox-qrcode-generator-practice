{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "18.2.0",
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "module": "ESNext",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "outDir": "dist"
    },
    "include": [
        "src"
    ]
}